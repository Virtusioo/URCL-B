
//setup:
    BITS == 16
    MINSTACK 8192
    MINHEAP 8192
    @define bp r20

//data:
    dw [80,69,69,80,69,69,0]
    imm r25 7 // heap base

//runtime:
    cal .main 
    hlt 
.main
    psh bp 
    mov bp sp 
    psh 8 
    mov r2 8 
    llod r1 bp -1 
    bne .L0_ r2 r1 
    psh 0 
    cal .puts 
    add sp sp 1 
.L0_
    mov sp bp 
    pop bp 
    ret 
